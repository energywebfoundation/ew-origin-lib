  "dependencies": {
    "axios": "^0.18.0",
    "curlrequest": "^1.0.1",
    "ethereumjs-tx": "^1.3.7",
    "ew-asset-registry-contracts": "v0.1.9",
    "ew-asset-registry-lib": "0.0.12-beta13",
    "ew-origin-contracts": "0.0.5-beta02",
    "ew-utils-general-lib": "v0.0.9-beta01",
    "ew-utils-testbackend": "0.0.3",
    "jsonschema": "^1.2.4",
    "precise-proofs": "0.0.1",
    "sloffle": "v1.3.0-beta06",
    "web3": "^1.0.0-beta.37",
    "winston": "^3.0.0",
    "write-json-file": "^2.3.0",
    "yaeti": "^1.0.2"
  },
