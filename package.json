    "@types/node": "^9.6.28",
    "@types/web3": "^1.0.14",
    "chai": "^4.1.2",
    "ew-erc-test-contracts": "0.0.2",
    "ew-user-registry-contracts": "0.0.9",
    "ganache-cli": "^6.1.8",
    "mocha": "^5.2.0",
    "solc": "^0.4.23",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "typescript-json-schema": "^0.31.0"
  },
  "postinstall": "rm -f node_modules/web3/index.d.ts"
